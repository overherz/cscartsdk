{
    "name": "cscart/sdk",
    "description": "Command-line tools for CS-Cart & Multivendor developers",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "cscart/upgrade-builder",
                "version": "master",
                "source": {
                    "url": "https://github.com/overherz/upgrade-builder",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "symfony/console": "~3.0",
        "symfony/filesystem": "~3.0",
        "ext-mbstring": "*",
        "overherz/upgrade-builder": "dev-master",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "Tygh\\": "src/"
        }
    },
    "bin": [
        "bin/cscart-sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Bolshakov",
            "email": "abolshakov@cs-cart.com"
        }
    ]
}
